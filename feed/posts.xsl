<?xml version="1.0" encoding="utf-8"?>
<!--

A template to extract each <item> in feed.xml to a standalone html file,
to be used to link to individual posts as new pages. This will replace the
"jump-link" action that currently happens with the `link` href.

-->

<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform"
  xmlns:atom="http://www.w3.org/2005/Atom" xmlns:dc="http://purl.org/dc/elements/1.1/" version="1.1">

  <xsl:output method="html" version="1.0" encoding="UTF-8" indent="yes" />

  <xsl:template match="/">

  </xsl:template>

</xsl:stylesheet>